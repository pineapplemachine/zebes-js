{
  "name": "zebes",
  "version": "0.1.1",
  "description": "Least of all build systems.",
  "keywords": [
    "c",
    "cpp",
    "c++",
    "d",
    "build",
    "linker",
    "compiler"
  ],
  "homepage": "https://github.com/pineapplemachine/zebes-js",
  "license": "MIT",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "readme.md",
    "changelog.md",
    "LICENSE",
    "package.json",
    "dist/src/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pineapplemachine/zebes-js.git"
  },
  "bugs": {
    "url": "https://github.com/pineapplemachine/zebes-js/issues"
  },
  "bin": {
    "zebes": "./dist/src/index.js"
  },
  "scripts": {
    "build": "./node_modules/.bin/tsc",
    "start": "node ./dist/src/index.js",
    "coverage": "nyc npm run test",
    "coveralls": "nyc --reporter text-lcov npm run test | coveralls",
    "test": "npm run build && node dist/test/test.js",
    "prepublish": "npm test"
  },
  "devDependencies": {
    "@types/node": "^16.11.1",
    "canary-test": "1.1.2",
    "coveralls": "^3.0.4",
    "nyc": "^15.1.0",
    "source-map-support": "0.5.12",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@iarna/toml": "^2.2.5",
    "argparse": "^2.0.1",
    "axios": "^0.23.0",
    "extract-zip": "^2.0.1",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "jaguar": "^6.0.1",
    "json5": "^2.2.0",
    "micromatch": "^4.0.4",
    "yaml": "^1.10.2"
  }
